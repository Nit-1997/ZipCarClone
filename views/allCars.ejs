<section class="bbc">
    <% if(error && error.length>0){%>
    <div class="container">
        <div class="alert alert-danger" role="alert">
            <%=error%>
        </div>
    </div>
    <% }%>
    <% if(success && success.length>0){%>
    <div class="container">
        <div class="alert alert-success" role="alert">
            <%=success%>
        </div>
    </div>
    <% }%>
    <div class="row" style="height: 722px;">
        <div class="col-md-3 col-sm-3 col-xs-12 ht500">
            <div class="row">
                <div class="col-md-3 col-sm-3 col-xs-12 ht161"></div>
                <div class="col-md-8 col-sm-8 col-xs-12 ht500 side">
                    <div class="ht161"></div>

                    <div class="input-group slide">
                        <!--<input type="text" class="form-control" placeholder="Search for pubs">-->
                        <p>
                        <form action="/multi" method="GET" class="form-inline">
                            <div class="form-group">
                                <input type="text" name="search" placeholder="search..." class="form-control">
                                <input type="submit" value="Filter Cars" class="btn btn-default">
                            </div>
                        </form>
                        </p>
                    </div>
                    <h4 class="slide"><strong>Filters</strong></h4>
                    <hr>
                    <p class="slide"><strong>Category filter:-</strong></p>
                    <div class="radio glide">
                        <label><input type="radio" name="optradio">GO</label>
                    </div>
                    <div class="radio glide">
                        <label><input type="radio" name="optradio">XL</label>
                    </div>
                    <div class="radio glide">
                        <label><input type="radio" name="optradio">Premium</label>
                    </div>
                    <hr>
                    <p class="slide"><strong>Fuel type filter:-</strong></p>
                    <div class="radio glide">
                        <label><input type="radio" name="optradio">Diesel</label>
                    </div>
                    <div class="radio glide">
                        <label><input type="radio" name="optradio">Petrol</label>
                    </div>
                    <div class="radio glide">
                        <label><input type="radio" name="optradio">Electric</label>
                    </div>
                    <div class="radio glide">
                        <label><input type="radio" name="optradio">Hybrid</label>
                    </div>
                </div>
                <div class="col-md-1 col-sm-1 col-xs-12 ht500"></div>
            </div>
        </div>
        <div class="col-md-8 col-sm-8 col-xs-12">
            <h1 class="pubs"><strong>Best Cars around you:-</strong></h1>
            <br><br>
            <% if(noMatch !== null) { %>
                <h3><%= noMatch %></h3>
            <% } %>
            <% cars.forEach(car => { %>
            <div class="event">
                <div class="row ht40">
                    <div class="col-md-4 col-sm-12 col-xs-12 ht40"><img class=newimg src="../assets/images/1.jpg"></div>
                    <div class="col-md-8 col-sm-12 col-xs-12 ht40 pubdet">
                        <h3 class="pubnm"><%=car.name%></h3>
                        <p class="lct"><strong><strong>Car Type : </strong><%=car.type%></strong></p>
                        <p class="st"><span class="cst"><strong>Rental Rate:</strong> </span> <strong><%=car.rentalRate%> </strong><span class="cst">$</span>
                        <p class="st"><span class="cst"><strong>Fuel Type:</strong> </span> <strong><%=car.fuelType%> </strong><span class="cst"></span>
<!--                        <p class="st"><span class="cst"><strong>Ratings -</strong></span><i class="fas fa-star"></i><i class="fas fa-star"></i><i class="fas fa-star"></i><i class="fas fa-star"></i></p>-->
                        <div class="btn btn-primary"><a href="multi/<%=car.id%>">Book</a></div>
                    </div>
                </div>
            </div>
            <%})%>


        </div>
        <div class="col-md-1 col-sm-1 col-xs-12 ht0"></div>
    </div>
</section>

